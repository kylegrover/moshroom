{
  "name": "moshroom",
  "version": "1.0.0",
  "description": "Datamosh GUI built in Electron",
  "main": "index.js",
  "dependencies": {
    "async": "^3.1.0",
    "electron": "^13.6.6",
    "python-shell": "^1.0.8"
  },
  "devDependencies": {
    "electron-packager": "^14.1.1",
    "ffbinaries": "^1.1.4"
  },
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . moshroom --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"m҉o҉s҉h҉r҉o҉o҉m҉\"",
    "package-linux": "electron-packager . moshroom --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "author": "�",
  "license": "ISC"
}
